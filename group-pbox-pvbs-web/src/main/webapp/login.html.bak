<body class="login-background">
<div class="ch-container">
        
    <div class="row">
        <div style="min-width:320px"  class="col-md-12 center login-header login-header" >
        		 
        		 		 <div style="min-width:320px" class="login-header-img">
        		 		 		<img  src="img/img-logo1.png" /><!--<span> SpiderNet1111111111111111111</span>-->
        		 		</div>
        		 	
        </div>
        		
           
        <!--/span-->
    </div><!--/row-->

    <div class="row">
        <div style="min-width:320px" class="well col-md-5 center login-box new-login-box">
    
            <div class="alert alert-info new-login-box-header">
                Welcome to SpiderNet
            </div>
            <form class="form-horizontal" action="index.html" method="post">
                <fieldset>
                    <div class="input-group input-group-lg">
                        <span  class="input-group-addon input-area-span"><i class="glyphicon glyphicon-user green-1"></i></span>
                        <input type="text"  class="form-control input-area" placeholder="用户名">
                    </div>
                    <div class="clearfix"></div><br>

                    <div class="input-group input-group-lg">
                        <span  class="input-group-addon input-area-span"><i class="glyphicon glyphicon-lock green-1"></i></span>
                        <input type="password"  class="form-control input-area" placeholder="密码">
                    </div>
                    <div class="clearfix"></div>

                   <!-- <div class="input-prepend">
                        <label class="remember" for="remember"><input type="checkbox" id="remember">  记住用户名</label>
                    </div>-->
                    <div class="clearfix"></div>

                    <p class="center col-md-5 p-style">
                        <button type="submit" class="btn btn-primary btn-primary-new">登录</button>
                    </p>
                </fieldset>
            </form>
        </div>
        <!--/span-->
    </div><!--/row-->
</div><!--/fluid-row-->


